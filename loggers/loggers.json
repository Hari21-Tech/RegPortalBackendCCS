{
  "version": 1,
  "disable_existing_loggers": true,
  "formatters": {
    "colored": {
      "()": "loggers.loggers.ColorFormatter"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "colored",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "pymongo": {
      "level": "WARNING",
      "propagate": false
    },
    "socketio": {
      "level": "INFO",
      "propagate": false
    },
    "asyncio": {
      "level": "WARNING",
      "propagate": false
    },
    "uvicorn": {
      "level": "DEBUG",
      "propagate": true
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": [
      "console"
    ],
    "propagate": false
  }
}
